    "\n",
    "data = fillbygroup(data, 'total_income')\n",
    "data.info() # снова вызываем метод и подтверждаем замену пропусков. \n",
    "data['days_employed'] = data['days_employed'].abs()\n",
    "data = fillbygroup(data, 'days_employed')\n",
